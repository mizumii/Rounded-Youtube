{
  "name": "rounded-youtube",
  "author": "mizumii",
  "description": "いたる所の角が丸くなったシンプルなYoutubeスキンです",
  "version": "1.0.0",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "content_scripts": [
    {
     "matches": ["https://www.google.co.jp/*"],
     "css": ["google/style.css"],"js": ["jquery.min.js","google/script.js"]
   },
    {
      "matches": ["https://www.youtube.com/*"],
      "css": ["youtube/style.css"],"js": ["youtube/script.js"]
    }
  ]
}
